      "api_config": {
        "anthropic_model": "claude-sonnet-4-5",
        "gemini_model": "gemini-pro",
        "webull_endpoint": "api.webull.com",
        "target_trade_latency_ms": 10
      }
    },
    {
      "id": 12,
      "name": "persistence_manager",
      "type": "memory-coordinator",
      "role": "Cross-session memory and mobile deployment",
      "domain": "persistence",
      "tasks": [
        "Persist AgentDB state across sessions",
        "Implement QUIC sync for distributed agents",
        "Build mobile deployment pipeline (iOS/Android)",
        "Enable edge execution for autonomous trading"
      ],
      "deployment_targets": [
        "desktop (Tauri + WASM)",
        "mobile (iOS + Android)",
        "edge (autonomous trading bots)"
      ]
    }
  ],
  "validation_schedule": {
    "hourly": [
      "agentdb reflexion store",
      "agentdb causal experiment add-observation",
      "agentdb skill consolidate --min-reward 0.9"
    ],
    "daily": [
      "agentdb learner run --min-confidence 0.85",
      "agentdb optimize-memory --compress true --consolidate-patterns true"
    ],
    "weekly": [
      "agentdb export --compress",
      "agentdb db stats",
      "Generate performance report"
    ]
  },
  "technology_stack": {
    "language": "Rust (100% - no TypeScript/JavaScript)",
    "ui_framework": "Tauri (cross-platform desktop + mobile)",
    "computation": "WASM (near-zero latency, data sovereignty)",
    "graphics": "egui + wgpu (pure Rust, no web dependencies)",
    "networking": "QUIC (quinn crate for distributed sync)",
    "math": "Integer-only (BinNet, Lucas, Latent-N logic)",
    "ai": "Multimodal thinking with φ-Mamba logic",
    "memory": "AgentDB (persistent learning)",
    "encoding": "Zeckendorf + Fibonacci + Lucas sets with massive φ/ψ recursion"
  },
  "ui_design": {
    "theme": "JARVIS-like holographic interface",
    "layout": "Positron-style IDE with floating panels",
    "visualization": "Real-time mind mapping of Zeckendorf bit cascades",
    "charts": "Computer vision AI-generated with golden ratio dynamics",
    "latency": "Sub-16ms frame time (60 FPS minimum)"
  }
}
