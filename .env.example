# Economic Data Pipeline - API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# ============================================
# FRED API (Federal Reserve Economic Data)
# ============================================
# Get your free API key from: https://fred.stlouisfed.org/docs/api/api_key.html
# Rate limit: 120 requests/minute
FRED_API_KEY=your_fred_api_key_here

# ============================================
# Alpha Vantage API (Optional)
# ============================================
# Get your free API key from: https://www.alphavantage.co/support/#api-key
# Free tier: 5 requests/minute, Premium: 75 requests/minute
ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here

# ============================================
# Database Configuration
# ============================================
# AgentDB path for persistent storage
AGENTDB_PATH=./data/aurelia.db

# Redis URL for caching (optional)
REDIS_URL=redis://localhost:6379

# ============================================
# Server Configuration
# ============================================
# API server port
PORT=3000

# Node environment (development | production | test)
NODE_ENV=development

# ============================================
# Rate Limiting
# ============================================
# Enable rate limiting for API endpoints
RATE_LIMIT_ENABLED=true

# Maximum requests per window
RATE_LIMIT_MAX_REQUESTS=1000

# Rate limit window in milliseconds (default: 60000 = 1 minute)
RATE_LIMIT_WINDOW_MS=60000

# ============================================
# Caching Strategy
# ============================================
# Enable caching (true | false)
CACHE_ENABLED=true

# Cache TTL in milliseconds (default: 300000 = 5 minutes)
CACHE_TTL=300000

# Cache strategy (memory | redis | agentdb)
CACHE_STRATEGY=memory

# ============================================
# WebSocket Configuration
# ============================================
# WebSocket server port for real-time updates
WS_PORT=3001

# Maximum WebSocket connections
WS_MAX_CONNECTIONS=1000

# ============================================
# Data Polling Intervals
# ============================================
# Market data polling interval in milliseconds (default: 15000 = 15 seconds)
MARKET_POLLING_INTERVAL=15000

# FRED data polling interval in milliseconds (default: 60000 = 1 minute)
FRED_POLLING_INTERVAL=60000

# Anomaly detection interval in milliseconds (default: 30000 = 30 seconds)
ANOMALY_POLLING_INTERVAL=30000

# ============================================
# Zeckendorf Encoding Configuration
# ============================================
# Maximum Fibonacci index to use in decomposition
ZECKENDORF_MAX_FIB_INDEX=50

# ============================================
# Monitoring & Logging
# ============================================
# Log level (debug | info | warn | error)
LOG_LEVEL=info

# Sentry DSN for error tracking (optional)
SENTRY_DSN=

# ============================================
# Security
# ============================================
# API authentication token (if required)
API_AUTH_TOKEN=

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# ============================================
# Feature Flags
# ============================================
# Enable Moody's risk scoring
ENABLE_MOODY_SCORING=true

# Enable GMV anomaly detection
ENABLE_GMV_SCORING=true

# Enable WebSocket streaming
ENABLE_WEBSOCKET=false

# Enable historical data storage
ENABLE_HISTORICAL_STORAGE=true

# ============================================
# Performance Tuning
# ============================================
# HTTP request timeout in milliseconds
HTTP_TIMEOUT=30000

# Maximum retry attempts for failed requests
MAX_RETRY_ATTEMPTS=3

# Retry delay in milliseconds
RETRY_DELAY=1000

# Batch processing chunk size
BATCH_CHUNK_SIZE=10

# ============================================
# Market Symbols Configuration
# ============================================
# Comma-separated list of symbols to track
# Default: SPY,QQQ,DIA,IWM,^VIX,GLD,TLT,USO,UUP,HYG
MARKET_SYMBOLS=SPY,QQQ,DIA,IWM,^VIX,GLD,TLT,USO,UUP,HYG

# ============================================
# FRED Series Groups to Enable
# ============================================
# Comma-separated list of groups to enable
# Options: INTEREST_RATES,INFLATION,EMPLOYMENT,GDP,MONEY_SUPPLY,CREDIT_MARKETS,CONSUMER,HOUSING,MANUFACTURING,ALL
FRED_ENABLED_GROUPS=INTEREST_RATES,INFLATION,EMPLOYMENT,MONEY_SUPPLY,CREDIT_MARKETS

# ============================================
# Notes
# ============================================
# - Never commit the actual .env file with real API keys to version control
# - Keep your API keys secure and rotate them regularly
# - Monitor your API usage to avoid hitting rate limits
# - Use environment-specific .env files (.env.development, .env.production)
# - Some features may require premium API subscriptions for higher rate limits
