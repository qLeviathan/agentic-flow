{
  "name": "agentdb-legal-assistant",
  "version": "1.0.0",
  "description": "AgentDB-powered legal assistant for pro se litigation - Castillo v. Schwab & Sedgwick",
  "main": "agentdb-legal-cli.js",
  "bin": {
    "agentdb-legal": "./agentdb-legal-cli.js"
  },
  "scripts": {
    "build": "tsc query-interface.ts --lib es2020 --target es2020 --module commonjs --esModuleInterop && tsc agentdb-legal-cli.ts --lib es2020 --target es2020 --module commonjs --esModuleInterop",
    "init": "./init-database.sh",
    "process-evidence": "node evidence-processor.js",
    "test": "node --test",
    "lint": "eslint *.ts"
  },
  "keywords": [
    "legal",
    "pro-se",
    "evidence-management",
    "agentdb",
    "vector-search",
    "litigation",
    "claude-ai"
