# Webull API Credentials
# Copy this file to .env and fill in your actual credentials

# Your Webull username (email)
WEBULL_USERNAME=your_email@example.com

# Your Webull password
WEBULL_PASSWORD=your_secure_password

# Device ID (generated automatically if not provided)
WEBULL_DEVICE_ID=your_device_id

# Multi-factor authentication code (if MFA is enabled)
WEBULL_MFA_CODE=123456

# Trading mode: Paper, CoPilot, or Autonomous
WEBULL_TRADING_MODE=Paper

# Logging level
RUST_LOG=info
