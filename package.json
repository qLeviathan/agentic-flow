{
  "name": "agentic-flow",
  "version": "2.0.0",
  "description": "Multi-agent coordination framework with QUIC transport",
  "main": "dist/index.js",
  "bin": {
    "math-framework": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:mvd": "tsc && cp src/chat_mvd.html dist/",
    "test": "jest",
    "test:phase-space": "jest tests/phase-space.test.ts",
    "test:validation": "jest tests/math-framework/validation",
    "test:aurelia": "ts-node scripts/run-validation-suite.ts",
    "test:aurelia:bootstrap": "jest tests/trading/aurelia",
    "test:aurelia:encoder": "jest tests/trading/core/state-encoder",
    "test:aurelia:cascade": "jest tests/trading/core/cascade-dynamics",
    "test:aurelia:data": "jest tests/trading/data",
    "test:aurelia:nash": "jest tests/trading/decisions",
    "test:aurelia:integration": "jest tests/integration/full-system",
    "test:aurelia:invariants": "jest tests/validation/consciousness-validation",
    "test:aurelia:coverage": "jest tests/trading tests/integration tests/validation --coverage",
    "demo:phase-space": "ts-node examples/phase-space-demo.ts",
    "demo:mvd": "npm run build:mvd && npm run mvd:server",
    "mvd:server": "ts-node examples/aurelia-mvd-server.ts",
    "mvd:serve": "npx serve src -p 3000",
    "graph:generate": "ts-node scripts/generate-dependency-graph.ts",
    "graph:render": "ts-node scripts/generate-dependency-graph.ts --render",
    "graph:svg": "ts-node scripts/generate-dependency-graph.ts --render --format=svg",
    "graph:pdf": "ts-node scripts/generate-dependency-graph.ts --render --format=pdf",
    "bench:quic": "node benchmarks/quic-transport.bench.js",
    "bench:report": "node scripts/generate-benchmark-report.js",
    "profile": "0x -- node benchmarks/quic-transport.bench.js",
    "lint": "eslint . --ext .ts,.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "quic",
    "multi-agent",
    "performance",
    "benchmark"
  ],
  "author": "qLeviathan",
  "contributors": [
    "Performance Benchmarker Agent",
    "AI Swarm Development Team"
  ],
  "license": "MIT",
  "dependencies": {
    "@fails-components/webtransport": "^0.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "agentdb": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "commander": "^11.1.0",
    "lucide-react": "^0.548.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^7.1.12",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "0x": "^5.6.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "typescript": "^5.9.3"
  }
}
